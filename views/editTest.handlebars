 <container class="editTestForm">
<header>
    <h1>Edit Test Information</h1>
    <form class="editTestForm">
    <div class="container editTestForm">
 <div class="test">
    <label for="testDate" class="col-form-label">Date Of Efficiency Test: </label>
    <div class="">
      <input type="date" class="form-control" id="testDate">
    </div>
  </div>
  <br>
  <div class="concepts">
    <label for="conceptsTested" class="col-form-label">Concepts Tested: </label>
    <div class="">
      <input type="input" class="form-control" id="conceptsTested" value={{test.concepts}}>
    </div>
  </div>
  <br>
  <div class="passed">
    <label for="passed" class="col-form-label">Passed: </label>
    <div class="">
      <input type="input" class="form-control" id="passed" value={{test.passed}}>
    </div>
  </div>
  <br>
  <div class="todo">
    <div class="offset-sm-2">
      <div class="form-check">
        <input class="form-check-input" type="checkbox" id="gridCheck1">
        <label class="form-check-label" for="gridCheck1">
           <p>To Do's: </p>
          Study for test based on job description
        </label>
      </div>
    </div>
  </div>
  <div class="">
    <div class="offset-sm-2">
      <div class="form-check">
        <input class="form-check-input" type="checkbox" id="gridCheck1">
        <label class="form-check-label" for="gridCheck1">
          Send Follow-Up Email
        </label>
      </div>
    </div>
  </div>
  <br>
<br><br>
  <button type="submit" class="btn btn-primary editSubmit" id="submitTest" value={{test.id}}>Submit</button>
  </form>
  </container>

<script src="/js/editTest.js"></script>